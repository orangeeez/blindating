{
    "version": "0.1.0",
    "showOutput": "silent",
    "tasks": [
        {
            "command": "tsc",
            "taskName": "tsc",
            "args": ["-p", "./src/Blindating/scripts"],
            "isBuildCommand": true,
            "isShellCommand": true,            
            "problemMatcher": "$tsc"
        },
        {
            "command": "dotnet",
            "taskName": "dotnet",
            "args": ["build", "src/Blindating/project.json"],
            "isBuildCommand": true,
            "isShellCommand": true,                        
            "problemMatcher": "$msCompile"
        },
    ]
}